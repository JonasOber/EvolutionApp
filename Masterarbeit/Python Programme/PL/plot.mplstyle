font.family : Arial
#figure.figsize : 3.74, 3.74 # figsize to 7 inches = 18 cm square, 5 inches = 12,7 cm, 9,5 cm = 3.74 inches (powerpoint)
#figure.autolayout : True # constrained layout true
axes.labelsize : 12# size of axes labeling
axes.linewidth: 1
lines.linewidth : 2
lines.markersize : 10
xtick.labelsize : 10
ytick.labelsize : 10
ytick.right : True
xtick.top : True
errorbar.capsize: 4 # length of endcap of errorbars